{
  "name": "av-lilybird-discord-bot",
  "description": "A custom discord bot",
  "version": "0.0.1",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "dev": "bun ./ci.ts --watch",
    "start": "bun .",
    "lint": "eslint \"**/*.ts\"",
    "check": "eslint \"**/*.ts\" --fix",
    "postinstall": "",
    "deps:check": "bunx taze major",
    "deps:update": "bunx taze major -w -i",
    "supabase:gen-types": "bunx supabase gen types typescript --db-url 'postgresql://postgres:postgres@127.0.0.1:54322/postgres' --schema public,discord > types/supabase.d.ts"
  },
  "dependencies": {
    "@linear/sdk": "^13.0.0",
    "@supabase/supabase-js": "^2.39.2",
    "av-database": "../database",
    "lilybird": "^0.4.4",
    "postgres": "^3.4.3",
    "tasai": "^0.1.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "bun-types": "latest",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "typescript": "^5.3.3"
  }
}
